    <pipelines>
      <initialize>
        <processor type="Trelleborg.Feature.CustomContainers.Pipelines.Initialize.InitializeRouting, Trelleborg.Feature.CustomContainers" resolve="true"  patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" />
